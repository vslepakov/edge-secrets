{
    "modulesContent": {
      "$edgeAgent": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "runtime": {
            "type": "docker",
            "settings": {
              "minDockerVersion": "v1.25",
              "loggingOptions": "",
              "registryCredentials": {
                "myRegistryName": {
                  "username": "$ACR_USER",
                  "password": "$ACR_PASSWORD",
                  "address": "$ACR_ADDRESS"
                }
              }
            }
          },
          "systemModules": {
            "edgeAgent": {
              "type": "docker",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
                "createOptions": "{}"
              }
            },
            "edgeHub": {
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
                "createOptions": "{}"
              },
              "env": {}
            }
          },
          "modules": {
            "sampleApp": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "${MODULES.SecretManagerInfluxdb}",
                "createOptions": {}
              },
              "env": {
                "EDGESECRET_KEYID": {
                  "value": "$EDGESECRET_KEYID"
                },
                "EDGESECRET_CRYPTO_PROVIDER": {
                  "value": "$EDGESECRET_CRYPTO_PROVIDER"
                },
                "EDGESECRET_INIT_VECTOR": {
                  "value": "$EDGESECRET_INIT_VECTOR"
                }
              }
            },
            "influxdb": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "influxdb:2.0",
                "createOptions": {
                  "HostConfig": {
                    "Binds":["/var/lib/influxdb/data:/var/lib/influxdb2","/var/lib/influxdb/config:/etc/influxdb2"]
                  }
                }
              },
              "env": {
                "DOCKER_INFLUXDB_INIT_MODE": {
                  "value": "setup"
                },
                "DOCKER_INFLUXDB_INIT_USERNAME": {
                  "value": "my-user"
                },
                "DOCKER_INFLUXDB_INIT_PASSWORD": {
                  "value": "my-password"
                },
                "DOCKER_INFLUXDB_INIT_ORG": {
                  "value": "my-org"
                },
                "DOCKER_INFLUXDB_INIT_BUCKET": {
                  "value": "my-bucket"
                }
              }
            }
          }
        }
      },
      "$edgeHub": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "routes": {
            "upstream": "FROM /messages/* INTO $upstream"
          },
          "storeAndForwardConfiguration": {
            "timeToLiveSecs": 7200
          }
        }
      }
    }
  }